<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.gt.entrypad">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:requestLegacyExternalStorage="true">
        <meta-data
            android:name="com.gt.entrypad.api.ApiConfigModule"
            android:value="ConfigModule" />
    <activity android:name=".module.project.ui.activity.ProjectListActivity"
        android:screenOrientation="landscape"
        android:theme="@style/MainAppTheme"
        android:windowSoftInputMode="adjustResize|stateHidden"/>
    <activity android:name=".module.project.ui.activity.ProjectHomePageActivity"
        android:screenOrientation="landscape"
        android:theme="@style/MainAppTheme"
        android:windowSoftInputMode="adjustResize|stateHidden"/>
    <activity android:name=".module.project.ui.activity.CameraActivity"
        android:screenOrientation="landscape"
        android:theme="@style/MainAppTheme"/>
        <activity android:name="com.zx.zxutils.views.PhotoPicker.PhotoPickerActivity"
            android:theme="@style/MainAppTheme"
            android:screenOrientation="landscape"
            />
        <activity android:name="com.zx.zxutils.views.PhotoPicker.PhotoPagerActivity"
            android:theme="@style/MainAppTheme"
            android:screenOrientation="landscape"/>

    </application>
</manifest>